body
{
	background-color: #949494;
	margin: 0;
	padding: 0;
	background: #aaa;
}


/*header*/
#wrapper {
	width: 860px;
	padding: 20px;
	background: #fff;
	margin: auto;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	margin-top: 20px;
	margin-bottom: 20px;
}
#header
{
	margin: 0 auto;
	padding: 0px;
	border-bottom: 0;
}
#header_text
{
	font-size: 20px;
	font-weight: bold;
	margin: 15px 3px;
	float: left;
	
}
#header_text a
{
	color: #222;
	text-decoration: none;
}
#header_search
{
	margin: 15px;
	float: right;
	margin-right: 0;
}
#header_search .field
{
	
}

#bg_img_wrapper {
	width: 900px;
	margin: auto;
}
#bg_img_wrapper {
	text-align: right;
}

#nav {
	height: 40px;
	background: #288b0a url(images/navi_bg.png) repeat-x;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	list-style: none;
	margin: 0;
	padding: 0;
	overflow: hidden;
	margin-bottom: 10px;
	margin-top: 10px;
}
#nav li {
	float: left;
	padding: 0;
	margin: 0;
}
#nav li ul {
	display: none;
} 
#nav li a {
	display: block;
	font-size: 16px;
	padding: 11px 20px;
	font-weight: normal;
	color: #222;
	text-decoration: none;
}
#nav li a:hover, #nav li a.active {
	text-decoration: none;
	background: #71ae15 url(images/navi_bg_hover.png) repeat-x;
}
#articles .article h2 a:hover {
	color: #639d0d !important;
}

#left
{
	width: 200px;
	float: left;
}



#left .box_new > .header .title {
	background: url(images/navi_bg.png) repeat-x;
	-webkit-border-top-left-radius: 5px;
	-webkit-border-top-right-radius: 5px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
	-moz-border-radius-topleft: 5px;
	-moz-border-radius-topright: 5px;
	padding: 10px;
	text-align: center;
	display: block;
}
#left .box_with_title > .content {
	background: #f5f5f5;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
#left .box_with_title > .content {
	padding: 10px;
}
/* left navigation */
#left .subbar
{
	list-style: none;
	margin: 0;
	padding: 0;
	margin-left: 5px;
	text-align: center;
}
#left .subbar li {
	border-bottom: 1px dashed #dfdfdf;
}
#left .subbar li a
{
	color: #000000;
	display: block;
	padding: 5px;
	text-decoration: none;
	font-weight: bold;
}
#left .subbar li.active
{
	background: #dfdfdf;
}
#left .subbar li.active ul
{
	margin-left: 10px;
}
#left .subbar li.active ul li a
{
	color: #5f5f5f;
}

#left .subbar li.active ul li a:hover
{
	background: #9f9f9f;
	color: #000000;
}
#left .subbar li.active ul li a.active
{
	background: #afafaf;
	color: #000000;
}
#left .subbar li a:hover
{
	background: #dfdfdf;
}


/* content area */
#content
{
		width: 630px;
		padding: 4px;
		padding-left: 14px;
		float: right;
		
}




#footer
{
	border-top: 1px solid #ccc;
	margin: 20px 5px 0px 5px;
	font-size: 8pt;
}
#footer a {
	font-size: 8pt;
}
#footer > div {
	padding: 10px 3px 3px 3px;
}
#footer_left {
	float: left;
}
#footer_right
{
	float: right;
}

#mobileswitcher {
	margin-top: 15px;
	text-align: center;
}


/*inputs*/
input[type=text], input[type=password],textarea
{
	border: 1px solid #9f9f9f;
	padding: 3px;
	font-weight: bold;
	font-family:Verdana,Lucida,sans-serif;
	color: #5f5f5f;
	background-color: #ffffff;
	width: 140px;
}





select
{
	padding: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	border: 1px solid #9f9f9f;
	margin: 3px;
}
select:focus
{
	-moz-box-shadow:0 0 8px #79b41b;
	-webkit-box-shadow:0 0 8px #79b41b;
	box-shadow:0 0 8px #79b41b;
	border-color:#2aa7ea !important;
}
textarea, input[type=text], input[type=password]
{
	padding: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	border: 1px solid #9f9f9f;
	margin: 3px;
}
textarea:focus, input[type=text]:focus, input[type=password]:focus
{
	-moz-box-shadow:0 0 8px #79b41b;
	-webkit-box-shadow:0 0 8px #79b41b;
	box-shadow:0 0 8px #79b41b;
	border-color:#79b41b !important;
	outline-width:0;
}
input[name=user]
{
	background-image:url(images/userpwd.png);
	background-position: 2px 2px;
	background-repeat:no-repeat;
	padding-left:20px;
}
input[name=pwd]
{
	background-image:url(images/userpwd.png);
	background-position:-15px -46px;
	background-repeat:no-repeat;
	padding-left:20px;
}

/* error */
.error
{
	/* error */
	background-color:#FFD7D7;
	border:1px dashed #CC0000;
	color:#313131;
	padding:9px;
}

/*box*/
.boxheader{
	/* boxheader */
	background: url(../../images/menu.png) repeat-x;
}




/*comments*/
.comments
{
	margin-top: 30px;
}
.comment
{
	background: #efefef;
	border-top: 2px solid #afafaf;
	padding: 5px;
	margin: 5px;
}
.comment .menu
{
	float: right;
}
.comment .from
{
	color: #4f4f4f;
}
.comment .text
{
	padding: 3px;
}

.member
{
	border-bottom: 1px solid #cfcfcf;
	padding: 1px;
	
}
.memberlist
{
	border-top: 1px solid #cfcfcf;
}
.member .data
{
	padding: 3px;
	margin: 2px;
	margin-left: 60px;
	font-weight: bold;
}
#inbox
{
	border-top: 3px double #efefef;
}
#inbox .ui-message
{
	border-bottom: 1px solid #efefef;
	padding: 3px;
	
}
#inbox .ui-body
{
	padding: 10px;
}
#inbox .ui-body .preview
{
	color: #9f9f9f;
}
#inbox  .delete
{
	padding: 15px;
}

#message_thread
{
	border-top: 3px double #efefef;
}
#message_thread .message .username
{
	font-weight: bold;
}
#message_thread .message .date
{	
	font-size: 11px;
	color: #9f9f9f;
}
#message_thread .message
{
	border-bottom: 1px solid #efefef;
	padding: 3px;
}
#message_thread .message .message-body
{
	padding: 5px 0px 5px 0px;
}

.searchbox .item
{
	margin: 3px;
}
.searchbox .item .title
{
	font-weight: bold;
	font-size: 13pt;
}

.searchbox .item .url
{
	color: #1069c0;
}

.search{}



