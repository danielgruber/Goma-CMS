			new Select("timezone", lang("timezone"), i18n::$timezones)
		), array(
			new FormAction("save", lang("save"))
		));
		$form->setSubmission("saveSettings");
		$form->addValidator(new RequiredFields(array("pagetitle")), "pagetitle");
		
