                if($this->getOwner()->search)
                {
                        if(_ereg('^(.*)\)\s*$', $query->addWhere, $matches))
                        {
                                $addwhere = $matches[1];
                                
                                $boxes = DataObject::_search("boxes", $this->getOwner()->search);
                                
                                
                                $_query = $boxes->buildQuery(array("seiteid"));
                                
                                // override default fields, because goma loads as default id, class_name, autorid and last_modfied
                                $_query->fields = array("seiteid" => "seiteid");
                                
                                
                                // now generate query and new addWhere
                                $addwhere .= ' OR `pages`.`id` IN ( '.$_query->build().' ) )';

                                $query->addWhere = $addwhere;
                        }
                }
                return $query;
        }
}


