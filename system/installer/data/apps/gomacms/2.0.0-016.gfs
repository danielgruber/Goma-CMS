(?<!DOCTYPE html>
<html lang="{$_cms_lang}">
	<head>
		<base href="{BASE_URI}" />
		<title>{$_cms_ptitle} <% IF $title %> - $title <% ELSE %> - {$_lang_administration} <% END %></title>
		
		<% INCLUDE_CSS "style.css" %>
		<% INCLUDE_CSS "box.css" %>
		<% INCLUDE_CSS "jqueryui/theme.css" %>
		
		<% INCLUDE_JS_MAIN "jquery" %>
		<% INCLUDE_JS_MAIN "js/loader.js" %>
		<script type="text/javascript">
			// <![CDATA[
				
				$(function(){
					$(window).resize(function(){
						$("#content > .header").css("min-width",0);
						$("#content > .header").css("min-width", $(document).width() - 220);
					});
					setInterval(function(){
						$("#content > .header").css("min-width",0);
						$("#content > .header").css("min-width", $(document).width() - 220);
					}, 2000);
					$("#content > .header").css("min-width",0);
					$("#content > .header").css("min-width", $(document).width() - 220);
					
				});
			// ]]>
		</script>
		{$header}
	</head>
	<body>
		<!--<div id="viewport">-->
			<div id="wrapper">
			
				<div id="leftbar">
					<div id="leftbar_inner">
						<img src="system/templates/admin/images/logo.png" alt="logo" />
						<div class="container">
							<span class="pagetitle">$_cms_ptitle</span>
						</div>
						<div class="container">
							<% lang("really welcome", "Welcome") %>, <strong>{$_cms_user}</strong>
							<div>[ <a href="profile/edit{URLEND}">{$_lang_edit_profile}</a> | <a href="pm/"><% lang("pm_inbox", "Inbox") %></a> ] </div><div> [ <a href="{ROOT_PATH}profile/logout/?redirect={$_SERVER_REQUEST_URI}"><% lang("logout", "logout") %></a> ]</div>
						</div>
						<div class="container">
							<span class="weblink"><a href="{ROOT_PATH}"><% lang("view_website", "Browse Website") %></a></span>
						</div>
						<div id="navi">
							<ul>
								<li>
									<% IF URL == "admin" %>
										<a href="{BASE_SCRIPT}admin/" class="active">
											<span><% lang("dashboard", "Dashboard") %></span>
										</a>
									<% ELSE %>
										<a href="{BASE_SCRIPT}admin/">
											<span><% lang("dashboard", "Dashboard") %></span>
										</a>
									<% END %>
									
								</li>
								<li>
									<% CONTROL this() %>
										<% IF $this.active %>
											<a class="active"  title="{$this.text}" href="{BASE_SCRIPT}admin/{$this.uname}{URLEND}">
												<span>$this.text</span>
											</a>
										<% ELSE %>
											<a title="{$this.text}" href="{BASE_SCRIPT}admin/{$this.uname}{URLEND}">
												<span>$this.text</span>
											</a>
										<% END %>
									<% ENDCONTROL %>
		
								</li>
							</ul>
						</div>
						<div id="langselect" class="container">
							<form method="get" action="">
								<select onchange="$(this).parent().submit();" id="langselect_page" name="setlang">
									<% CONTROL languages() AS $lang %>
										<% IF $lang.active %>
											<option value="$lang.name" selected="selected">$lang.title</option>
										<% ELSE %>
											<option value="$lang.name">$lang.title</option>
										<% END %>
										
									<% ENDCONTROL %>
								</select>
							</form>
						</div>
						<div id="footer">
							Powered by <a href="http://goma-cms.org" target="_blank">Goma</a>
						</div>
					</div>
				</div>
				<garea name="content" reload="1">
					<div id="content">
						<% IF $content %>
							<div class="header">
								<h1>$title.text()</h1>
							</div>	
							<div class="content_inner">
								$addcontent
								$content
							</div>
						<% ELSE %>
							<div class="header">
								<h1><% lang("dashboard", "Dashboard") %></h1>
							</div>	
							<div class="content_inner">
								<div id="addcontent" style="width: 900px;max-height: 150px;overflow: auto;">
									$addcontent
								</div>
								<div id="statistics">
									<div class="title">
										<% lang("visitors", "Visitors") %>
									</div>
									<div class="controls">
										<span style="color: #24acb8;">&middot;</span> <a style="color: #24acb8;" href="{BASE_SCRIPT}{URL}{URLEND}"><% lang("visitors_by_day", "By Day") %></a> <span style="color: #da097a;">&middot; </span> <a style="color: #da097a;" href="{BASE_SCRIPT}{URL}{URLEND}?month=1"><% lang("visitors_by_month", "By Month") %></a>
									</div>
									<% IF $_GET_month %>
										<% CONTROL statistics() %>
											<div class="stat month">
												<div class="data">
													<div style="height:<% echo $statistics.percent*2 %>px">
													</div>
												</div>
												<div>$statistics.count</div>
												<div>$statistics.timestamp.date("F")</div>
											</div>
										<% ENDCONTROL %>
									<% ELSE %>
										<% CONTROL statistics(false) %>
											<div class="stat">
												<div class="data">
													<div style="height:<% echo $statistics.percent*2 %>px">
													</div>
												</div>
												<div>$statistics.count</div>
												<div>$statistics.timestamp.date("j M")</div>
											</div>
										<% ENDCONTROL %>
									<% END %>
									<form action="" method="get">
										
									</form>
									<div class="clear"></div>
								</div>
								<script type="text/javascript">
									var update_url = "http://download.goma-cms.org/api/v1/gomadownloads.json?version={GOMA_VERSION}-{BUILD_VERSION}&add_fields=UpdateFile,UpdateVersion";
									$(function(){
										$("#loading").css("display", "block");
										$.ajax({
											dataType: "jsonp",
											url: update_url,
											success: function(data)
											{
												if(data.data[0] == null)
												{
													$("#update_response").html('<span style="color: #00aa00;">{$_lang_version_current}</span>');
												} else
												{
														if(data.data[0].UpdateFile === false)
														{
															$("#update_response").html('<span style="color: #00aa00;">{$_lang_version_current}</span>');
														} else
														{
															$("#update_response").html('<div style="color: #cc0000;">{$_lang_version_available}</div> <a href="http://download.goma-cms.org/download_goma/update/{GOMA_VERSION}-{BUILD_VERSION}">{$_lang_upgrade_to_next}</a>');
														}
												}
												$("#loading").css("display", "none");
											}
										});
									});
								</script>
								<div id="home-container">
									
									<div id="left">
										<div id="version" class="content_container">
											<h2>Goma {$_lang_version}</h2>
											<div>
												<div>{$_lang_version}: {GOMA_VERSION} - {BUILD_VERSION}</div>
												<div id="loading" style="display: none;">Checking updates ...</div>
												<div>
													<div id="update_response"></div>
												</div>
											</div>
										</div>
										<div id="cache" class="content_container">
											<h2>{$_lang_del_cache}</h2>
											<div>
												<div class="info">{$_lang_cache_del_info}</div>
												<a href="admin/?flush=1" class="button">{$_lang_del_cache}</a>
											</div>
										</div>
									</div>
									<div id="right">
									
										<div id="database" class="content_container">
											<h2>{$_lang_database}</h2>
											<div>
												<p class="info">{$_lang_db_update_info}</p>
												<a href="dev" rel="ajaxfy">{$_lang_db_update}</a>
											</div>
										</div>
									</div>
									
									<div class="clear"></div>
									
									
								</div>
							</div>
						<% END %>
					
				
					</div>
				</garea>
			
			</div>
		<!--</div>-->
	</body>
</html><% INCLUDE_CSS "boxes.css" %>
<% IF permission("boxes_all") AND NOT adminAsUser() %>
	<% GLOAD("sortable") %>
	<% GLOAD("resizable") %>
	<% GLOAD("dropdownDialog") %>
	<script type="text/javascript">
		// <![CDATA[
			(function($){
				$(function(){
					$("#boxes_new_$id .box_new").resizable({
						autoHide: true,
						handles: 'e',
						minWidth: 100,
						grid: [10, 10],
						stop: function(event, ui) {
							$.ajax({
								url: root_path + "boxes_new/$id/saveBoxWidth/" + ui.element.attr("id").replace("box_new_", ""),
								type: "post",
								data: {width: ui.element.width()},
								dataType: "html"
							});
						},
						resize: function(event, ui){
							ui.element.css('height','auto');
						}
					});
					$("#boxes_new_$id").sortable({
						opacity: 0.6,
						handle: '.adminhead',
						helper: 'clone',
						placeholder: 'placeholder',
						revert: true,
						tolerance: 'pointer',
						start: function(event, ui) {
							$(".placeholder").css({'width' : ui.item.width(), 'height': ui.item.height()});
							$(".placeholder").attr("class", ui.item.attr("class") + " placeholder");
							
						},
						update: function(event, ui) {
							var data  = $(this).sortable("serialize");
							// save order
							$.ajax({
								url: root_path + "boxes_new/$id/saveBoxOrder",
								data: data,
								type: "post",
								dataType: "html"
							});
						},
						distance: 10,
						items: " > .box_new"
					});
					$("#boxes_new_$id > .box_new .adminhead").css("cursor", "move");
				});
			})(jQuery);
		// ]]>
	</script>
<% END %>

<div class="boxes_new" id="boxes_new_$id">
	<% IF $this %>
		<% CONTROL this() AS $box %>
			<% IF permission("boxes_all") AND NOT adminAsUser() AND $box.title %>
				<div class="box_new adminview {$box.class_name} box_with_title $box.border_class" style="width: {$box.width}px;" id="box_new_{$box.id}">
			<% ELSE IF permission("boxes_all") AND NOT adminAsUser() %>
				<div class="box_new adminview {$box.class_name} $box.border_class" style="width: {$box.width}px;" id="box_new_{$box.id}">
			<% ELSE IF $box.title %>
				<div class="box_new {$box.class_name} box_with_title $box.border_class" style="width: {$box.width}px;" id="box_new_{$box.id}">
			<% ELSE %>
				<div class="box_new {$box.class_name} $box.border_class" style="width: {$box.width}px;" id="box_new_{$box.id}">
			<% END %>
				<% IF permission("boxes_all") AND NOT adminAsUser() %>
					<div class="adminhead">
						<div class="actions">
							<a href="boxes_new/$id/add?insertafter={$box.sort}&redirect=$_SERVER_REDIRECT.URL()" title="{$_lang_new_box}" rel="dropdownDialog">
								<img src="images/16x16/add.png" alt="{$_lang_new_box}" />
							</a>
							
							<a class="noAutoHide" href="boxes_new/$id/edit/{$box.id}?redirect=$_SERVER_REDIRECT.URL()" title="{$_lang_edit_box}" rel="dropdownDialog">
								<img src="images/16x16/edit.png" alt="{$_lang_edit_box}" />
							</a>
							
							<a class="" href="boxes_new/$id/delete/{$box.id}?redirect=$_SERVER_REDIRECT.URL()" title="{$_lang_del_box}" rel="dropdownDialog">
								<img src="images/16x16/del.png" alt="{$_lang_del_box}" />
							</a>
						</div>
					</div>
				<% END %>
				
				<% IF $box.title %>
					<div class="header">
						<span class="title">$box.title</span>
					</div>
				<% END %>
				
				<div class="content">
					$box.content
				</div>
			</div>
		<% ENDCONTROL %>
	<% ELSE %>
		<% IF permission("boxes_all") AND NOT adminAsUser()  %>
			<div class="no_box">
				<a rel="dropdownDialog" href="boxes_new/$id/add?insertafter=1">{$_lang_new_box}</a>
			</div>
		<% END %>
	<% END %>
