		$data = DataObject::get("newsettings", array("id" => 1));
		$data->titel = $result["pagetitle"];
		$data->timezone = $result["timezone"];
		$data->write(false, true);
		HTTPResponse::redirect(BASE_URI . BASE_SCRIPT . "finish/");
	}
