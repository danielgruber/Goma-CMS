#wrapper {
	width: 91%;
	margin: auto;
	margin-top: 20px;
	padding: 3%;
	margin-bottom: 20px;
}
.box {
	width: 100%;
}
#header_search
{
	float: none;
	margin: 0;
}

#content
{
		width: 100%;
		padding: 0;
		margin: 0;
		padding-top: 5px;
}
#content img {
	max-width: 98%;
}

#mobilenavi, #mobilenavi ul {
	margin: 0;
	padding: 0;
	padding-left: 20px;
	list-style: none;
	background: #fff
}
#mobilenavi {
	border-bottom: 1px solid #ccc;
	border-top: 1px solid #ccc;
	margin-top: 10px;
	margin-bottom: 10px;
}


#mobilenavi li a {
	display: block;
	padding: 5px;
	;
	color: #2f2f2f;
}
#mobilenavi li a:hover, #mobilenavi li a.active {
	background: #fff;
	color: #2f2f2f;
font-weight: bold;
}
